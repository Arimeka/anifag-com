.row
  - cache action: 'index', action_suffix: 'article_feed' do
    .col-lg-9.col-md-9.articles
      .posts-feed
        = render partial: 'articles/article_feed', collection: @articles_other, as: :article
      .row.more
        .col-sm-6.col-sm-offset-3
          = link_to t('label.articles.more'), root_path, class: 'btn btn-primary btn-lg btn-block', id: 'load-more'
          .spinner.hide
            .rect1
            .rect2
            .rect3
            .rect4
            .rect5
      .row
        .col-md-12.hidden-sm.hidden-xs
          .js-banner#home-feed-footer-banner{data: {max_width: '840px'}}
  - cache action: 'index', action_suffix: 'aside' do
    %aside.col-lg-3.col-md-3
      .row
        .col-md-12
          .js-banner#home-aside-banner-top{data: {max_width: '290px'}}
      - cache 'aside_video_block' do
        = render 'shared/blocks/aside_video_block'
      - cache 'aside_gallery_block' do
        = render 'shared/blocks/aside_gallery_block'
