- content_for :title, "#{t('activerecord.models.article.title')} - Anime Fag"

.row
  .col-lg-9
    .row.posts-feed
      - 30.times do
        %article.col-lg-4.col-sm-4.col-xs-12
          = link_to article_path(1) do
            = image_tag 'dummy/250x211.png', class: 'img-responsive', size: '250x211'
            %h4.title Lorem ipsum dolor sit amet, consectetur adipisicing elit
  %aside.col-lg-3
    = render 'shared/blocks/aside_video_block'
    = render 'shared/blocks/aside_gallery_block'

